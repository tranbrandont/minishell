envset N 1
echo N is ${N}.
echo N + 1 is $(expr ${N} + 1)
echo Without environment variables that is ... $(expr 1 + 1)
echo Two in one ... $(expr 2 + 2) and $(expr 3 + 3)
aecho Testing last character issues.
aecho $(echo -n Testing.)
aecho "Testing built-in commands in dollar paren expansions."
aecho '${Q}' $(envset Q 13) '${Q}'
aecho Q should still be 13 ... Q is ${Q}
envset R "$(aecho From aecho)"
aecho Environment var R is '${R}' and should have something.
aecho '${R}' now?
aecho '${R}' now?
touch -m -t200505010000 file
chmod 542 file
envset R "$(sstat file)"
aecho Environment var R is '${R}' and should have something.
aecho Testing exit values from command expansion
echo true $(true) $? false $(false) $? should be true 0 false 1
echo Multiple commands ..
echo $(expr $(expr 3 + 5) \* $(expr 2 \* $(expr 5 + $(expr 10 \* 20))))
echo Doing a big one ... may need to send ^C ...
gcc ${TOP}/argcnt.c
./a.out $(cat ${TOP}/bigfile)
rm a.out

